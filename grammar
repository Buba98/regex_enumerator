REGEX := REGEX_ALT '|' REGEX_ALT | REGEX_ALT
REGEX_ALT := GROUPING REGEX_ALT | GROUPING | CHAR_CLASSES REGEX_ALT | CHAR_CLASSES
GROUPING := '(' REGEX ')'
CHAR_CLASSES := CHAR_CLASS QUANTIFIER | CHAR_CLASS
QUANTIFIER := '*' | '+' | '?' | '{' NUMBER '}' | '{' NUMBER ',' NUMBER '}' | '{' NUMBER ',}'
CHAR_CLASS := POSITIVE_CHAR_CLASS | NEGATIVE_CHAR_CLASS | CHAR | '.'
POSITIVE_CHAR_CLASS := '[' CHAR_CLASS_ITEMS ']'
NEGATIVE_CHAR_CLASS := '[^' CHAR_CLASS_ITEMS ']'
CHAR_CLASS_ITEMS := CHAR_CLASS_ITEM CHAR_CLASS_ITEMS | CHAR_CLASS_ITEM
CHAR_CLASS_ITEM := CHAR '-' CHAR | CHAR | ESCAPED_CHAR